{
    "name": "nikaia/nova-rating-field",
    "description": "Add start rating field to Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "rating"
    ],
    "homepage": "https://github.com/nikaia/nova-rating-field",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require-dev": {
        "laravel/nova": "^4.2",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^7.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "authors": [
        {
            "name": "Nassif Bourguig",
            "email": "nbourguig@gmail.com",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "Nikaia\\Rating\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikaia\\Rating\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikaia\\Rating\\RatingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
